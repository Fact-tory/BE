# 서버 설정
HOST=0.0.0.0
PORT=8000
DEBUG=false

# RabbitMQ 설정
RABBITMQ_URL=amqp://localhost:5672
RABBITMQ_HOST=localhost
RABBITMQ_PORT=5672
RABBITMQ_USERNAME=guest
RABBITMQ_PASSWORD=guest
RABBITMQ_VHOST=/
RABBITMQ_ENABLED=true

# Java 백엔드 설정
JAVA_BACKEND_URL=http://localhost:8080
JAVA_BACKEND_HEALTH_CHECK=/actuator/health
JAVA_BACKEND_TIMEOUT=5

# 크롤링 설정
MAX_CONCURRENT_CRAWLS=3
CRAWLING_DELAY=0.5
PAGE_TIMEOUT=30000

# 브라우저 설정
BROWSER_HEADLESS=true

# 로깅 설정
LOG_LEVEL=INFO
LOG_FORMAT=json

# 저장 설정
OUTPUT_DIR=./output
SAVE_RAW_DATA=true